{
    "version": 1,
    "project": "tardis",
    "project_url": "https://tardis-sn.github.io/tardis",
    "repo": ".",
    "pythons": ["/home/runner/micromamba/envs/tardis/bin/python"],
    "build_command": [
        "pip install -e .",
        "PIP_NO_BUILD_ISOLATION=false python -mpip wheel --no-deps --no-index -w {build_cache_dir} {build_dir}"
    ],
    "branches": [
        "origin/master",
        "HEAD"
    ],
    "environment_type": "mamba",
    "show_commit_url": "https://github.com/tardis-sn/tardis/commit/",
    "conda_environment_file": "IGNORE",
    "benchmark_dir": "benchmarks",
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html",
    "matrix": {
        "env": {
            "TARDIS_REF_PATH": "tardis-regression-data/atom_data"
        }
    }
}